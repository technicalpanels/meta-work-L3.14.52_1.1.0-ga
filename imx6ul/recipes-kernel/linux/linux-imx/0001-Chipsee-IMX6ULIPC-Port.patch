+static u8 ascii2hex(u8 a)
+{
+	s8 value = 0;
+	if (a >= '0' && a <= '9') {
+		value = a - '0';
+	} else if (a >= 'A' && a <= 'F') {
+		value = a - 'A' + 0x0A;
+	} else if (a >= 'a' && a <= 'f') {
+		value = a - 'a' + 0x0A;
+	} else {
+		value = 0xff;
+	}
+	return value;
+}
