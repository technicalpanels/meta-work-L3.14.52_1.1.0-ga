+void gup_leave_update_mode(void)
+{
+    GTP_GPIO_AS_INT(gtp_int_gpio);
+    
+    GTP_DEBUG("[leave_update_mode]reset chip.");
+    gtp_reset_guitar(i2c_connect_client, 20);
+}
