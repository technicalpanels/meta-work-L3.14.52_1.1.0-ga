+void gtp_get_chip_type(struct goodix_ts_data *ts)
+{
+    u8 opr_buf[10] = {0x00};
+    s32 ret = 0;
+    
+    msleep(10);
+    
+    ret = gtp_i2c_read_dbl_check(ts->client, GTP_REG_CHIP_TYPE, opr_buf, 10);
