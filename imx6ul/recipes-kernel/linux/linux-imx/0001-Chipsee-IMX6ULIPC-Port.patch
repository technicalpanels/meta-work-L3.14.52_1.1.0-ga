+// main clock calibration
+// bit: 0~7, val: 0/1
+static void gup_bit_write(s32 addr, s32 bit, s32 val)
+{
+    u8 buf;
+    i2c_read_bytes(i2c_connect_client, addr, &buf, 1);
+
+    buf = (buf & (~((u8)1 << bit))) | ((u8)val << bit);
+
+    i2c_write_bytes(i2c_connect_client, addr, &buf, 1);
+}
