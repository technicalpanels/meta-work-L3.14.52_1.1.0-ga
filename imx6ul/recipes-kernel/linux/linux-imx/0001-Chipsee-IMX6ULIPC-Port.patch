+		input_report_abs(input_dev, ABS_MT_PRESSURE, size);
+		input_report_abs(input_dev, ABS_MT_TOUCH_MAJOR, size);
+		input_report_abs(input_dev, ABS_MT_TRACKING_ID, id);
