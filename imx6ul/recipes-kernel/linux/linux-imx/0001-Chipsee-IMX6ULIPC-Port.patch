+void gt1x_touch_down(s32 x, s32 y, s32 size, s32 id)
+{
+#if GTP_CHANGE_X2Y
+	GTP_SWAP(x, y);
+#endif
+#if GTP_SINGLETOUCH
+    input_report_abs(input_dev, ABS_X, x);
