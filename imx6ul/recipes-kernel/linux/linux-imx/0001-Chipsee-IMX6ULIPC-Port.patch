+void gt1x_select_addr(void)
+{
+	GTP_GPIO_OUTPUT(GTP_RST_PORT, 0);
+	GTP_GPIO_OUTPUT(GTP_INT_PORT, gt1x_i2c_client->addr == 0x14);
+	msleep(2);
+	GTP_GPIO_OUTPUT(GTP_RST_PORT, 1);
+    msleep(2);
+}
