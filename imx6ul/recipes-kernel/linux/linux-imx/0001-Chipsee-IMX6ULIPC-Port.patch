+static s32 gt1x_set_reset_status(void)
+{
+    /* 0x8040 ~ 0x8043 */
+	u8 value[] = {0xAA, 0x00, 0x56, 0xAA};
+    int ret;
+    
+	GTP_DEBUG("Set reset status.");
+    ret = gt1x_i2c_write(GTP_REG_CMD + 1, &value[1], 3);
+    if (ret < 0)
+        return ret;
+
+    return gt1x_i2c_write(GTP_REG_CMD, value, 1);
+}
