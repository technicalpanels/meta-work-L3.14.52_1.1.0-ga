From e6e13fe5f01989d9dbc6c5e35eb5113c4088e81c Mon Sep 17 00:00:00 2001
From: Xiaoqiang Liu <leavs_o@126.com>
Date: Wed, 8 May 2019 18:37:06 +0800
Subject: [PATCH] Update LCD clock frequency [45MHz] and pol [1]

---
 arch/arm/boot/dts/mx6ulipc.dts | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/mx6ulipc.dts b/arch/arm/boot/dts/mx6ulipc.dts
index 06db59f..7cf21e9 100644
--- a/arch/arm/boot/dts/mx6ulipc.dts
+++ b/arch/arm/boot/dts/mx6ulipc.dts
@@ -167,7 +167,7 @@
 		display-timings {
 			native-mode = <&timing0>;
 			timing0: timing0 {
-				clock-frequency = <42400000>;
+				clock-frequency = <51200000>;
 				hactive = <1024>;
 				vactive = <600>;
 				
@@ -182,7 +182,7 @@
 				hsync-active = <0>;
 				vsync-active = <0>;
 				de-active = <1>;
-				pixelclk-active = <1>;
+				pixelclk-active = <1>;
 			};
 		};
 	};
-- 
2.7.4

