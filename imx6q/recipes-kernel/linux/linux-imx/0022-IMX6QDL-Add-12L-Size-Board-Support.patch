+/ {
+	backlight {
+	        compatible = "pwm-backlight";
+	        pwms = <&pwm4 0 5000000>;
+        	brightness-levels = <0 4 8 16 32 64 128 255>;
+	        default-brightness-level = <7>;
+		pwm-invert = <1>;
+        	status = "okay";
+	};
+};
+
+&ldb {
+	status = "okay";
+
+	lvds-channel@0 {
+                fsl,data-mapping = "spwg";
+                fsl,data-width = <24>;
+                primary;
+                status = "okay";
+
+		display-timings {
+                        native-mode = <&timing012>;
+			timing012: AUO120 {
+                                clock-frequency = <40000000>;
+                                hactive = <800>;
+                                vactive = <600>;
+                                hback-porch = <127>;
+                                hfront-porch = <127>;
+                                vback-porch = <32>;
+                                vfront-porch = <32>;
+                                hsync-len = <2>;
+                                vsync-len = <2>;
+                        };
+		};	
+	};
+
+	lvds-channel@1 {
+                status = "disabled";
+	};
+};
+
+&pwm4 {
+        pinctrl-names = "default";
+        pinctrl-0 = <&pinctrl_pwm4>;
+        status = "okay";
+};
