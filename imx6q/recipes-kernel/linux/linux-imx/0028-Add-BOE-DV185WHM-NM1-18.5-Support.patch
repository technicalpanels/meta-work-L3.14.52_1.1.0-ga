+/ {
+	backlight {
+	        compatible = "pwm-backlight";
+	        pwms = <&pwm4 0 5000000>;
+        	brightness-levels = <0 4 8 16 32 64 128 255>;
+	        default-brightness-level = <7>;
+		pwm-invert = <1>;
+        	status = "okay";
+	};
+};
+
+&ldb {
+	status = "okay";
+
+	lvds-channel@0 {
+                fsl,data-mapping = "spwg";
+                fsl,data-width = <24>;
+                primary;
+                status = "okay";
+
+		display-timings {
+                        native-mode = <&timing018>;
+			timing018: BOE185 {
+                                clock-frequency = <75441600>;
+                                hactive = <1366>;
+                                vactive = <768>;
+                                hback-porch = <50>;
+                                hfront-porch = <50>;
+                                vback-porch = <10>;
+                                vfront-porch = <10>;
+                                hsync-len = <94>;
+                                vsync-len = <18>;
+                        };
+		};	
+	};
+
+	lvds-channel@1 {
+                status = "disabled";
+	};
+};
+
+&pwm4 {
+        pinctrl-names = "default";
+        pinctrl-0 = <&pinctrl_pwm4>;
+        status = "okay";
+};
+
+&i2c1 {
+	tsc2007@48 {
+		has-noise = <1>;
+	};
+};
