+/ {
+	backlight {
+	        compatible = "pwm-backlight";
+	        pwms = <&pwm4 0 5000000>;
+        	brightness-levels = <0 4 8 16 32 64 128 255>;
+	        default-brightness-level = <7>;
+		pwm-invert = <0>;
+        	status = "okay";
+	};
+};
+
+&ldb {
+	status = "okay";
+	split-mode;
+	lvds-channel@0 {
+                fsl,data-mapping = "spwg";
+                fsl,data-width = <24>;
+                primary;
+                status = "okay";
+
+		display-timings {
+                        native-mode = <&timing0>;
+			timing0: CHIMEI-WXGA+ {
+				clock-frequency = <88800000>;
+                                hactive = <1440>;
+                                vactive = <900>;
+                                hback-porch = <60>;
+                                hfront-porch = <60>;
+                                vback-porch = <20>;
+                                vfront-porch = <20>;
+                                hsync-len = <40>;
+                                vsync-len = <12>;
+                        };
+		};	
+	};
+
+	lvds-channel@1 {
+		fsl,data-mapping = "spwg";
+                fsl,data-width = <24>;
+                status = "okay";
+
+		display-timings {
+			native-mode = <&timing1>;
+                        timing1: CHIMEI-WXGA+  {
+				clock-frequency = <88800000>;
+                                hactive = <1440>;
+                                vactive = <900>;
+                                hback-porch = <60>;
+                                hfront-porch = <60>;
+                                vback-porch = <20>;
+                                vfront-porch = <20>;
+                                hsync-len = <40>;
+                                vsync-len = <12>;
+                        };
+		};
+	};
+};
+
+&pwm4 {
+        pinctrl-names = "default";
+        pinctrl-0 = <&pinctrl_pwm4>;
+        status = "okay";
+};
