+#include "imx6dl-eisd.dts"
+#include "imx6qdl-eisd-1024768-97.dtsi"
+
+&i2c1 {
+        goodix_ts@5d {
+                compatible = "goodix,gt9xx";
+                reg = <0x5d>;
+                interrupt-parent = <&gpio1>;
+                interrupts = <9 2>;
+                goodix,irq-gpio = <&gpio1 9 0>;
+                goodix,rst-gpio = <&gpio5 20 0>;
+                goodix,cfg-group0 = [
+                        41 00 04 00 03 0A 0D 00 01 08 28
+                        05 50 32 03 05 00 00 00 00 00 00
+                        00 00 00 00 00 8E 2E 88 17 15 31
+                        0D 00 00 01 BA 03 1D 00 00 00 00
+                        00 00 00 00 00 00 00 0F 2C 94 C5
+                        02 07 00 00 04 A3 10 00 86 14 00
+                        6F 19 00 5B 20 00 4F 27 00 4F 00
+                        00 00 00 00 00 00 00 00 00 00 00
+                        00 00 00 00 00 00 00 00 00 00 00
+                        00 00 00 00 00 00 00 00 00 00 00
+                        00 00 15 14 11 10 0F 0E 0D 0C 09
+                        08 07 06 05 04 01 00 FF FF FF FF
+                        00 00 00 00 00 00 00 00 00 00 00
+                        02 04 06 07 08 0A 0C 0D 0F 10 11
+                        12 13 19 1B 1C 1E 1F 20 21 22 23
+                        24 25 26 27 28 FF FF FF FF 00 00
+                        00 00 00 00 00 00 00 00 9E 01
+                ];
+        };
+};
+
